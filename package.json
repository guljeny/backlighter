{
  "name": "backlighter",
  "version": "1.0.0",
  "description": "backlighter",
  "main": "app/index.js",
  "scripts": {
    "start-dev": "NODE_ENV=development npm run server && npm run mongod && npx webpack --watch",
    "start": "NODE_ENV=production PORT=80 pm2 start ecosystem.config.js && npm run mongod",
    "server": "pm2 start ecosystem.config.js",
    "mongod": "mongod --config ./mongod.conf  --fork --logpath /dev/null",
    "build": "NODE_ENV=production npx webpack -p",
    "lint": "npx eslint ./src && npx eslint ./app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jenya Gul",
  "license": "ISC",
  "_moduleAliases": {
    "~root": ".",
    "$config": "./src/config.js",
    "$utils": "./src/utils",
    "$db": "./src/db.js",
    "$app": "./app"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongodb-session": "^2.3.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "install": "^0.13.0",
    "js-md5": "^0.7.3",
    "module-alias": "^2.2.2",
    "mongodb": "^3.5.6",
    "npm": "^6.14.4",
    "react": "^16.13.1",
    "require-yml": "^2.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
